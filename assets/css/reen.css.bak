.btn{
	padding:30px;
	background-color:#00bc9c;
	cursor:pointer;
	color:#f5f7fa;
	margin:0 auto;
	text-align:center;
	border:2px solid #00bc9c;
	border-radius:4px;
	
}
.btn:active,
.btn:focus,
.btn:hover,
.btn:visited{
	background-color:#f5f7fa;
	color:#00bc9c;
}
.btn2{
	padding:20px 10px;
	margin:0 auto;
	color:#f5f7fa;
