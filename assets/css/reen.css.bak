*{
padding:0px;
margin:0px;
box-sizing:border-box;
}


/* helper class */
a:link,
a:hover,
a:focus,
a:active{
	text-decoration:none;
	list-style:none;
}
.container{
	width:1170px;
	margin:0 auto;
	
}
.row,
.row::after,
.row::before{
	display:table;
	clear:both;
	width:100%;
	
	
}
.row > :first-child{
	padding-left:0;
	
}
.row > :last-child{
	padding-right:0;
	
}
.col6,.col2,.col12,.col4,.col3,.col9,.col8{
	
	float:left;
	padding:0 15px;
}
.col6{
	width:50%;
}
.col8{
	width:calc(800% / 12);
}
.text-right{
	text-align:right;
}
.text-center{
	text-align:center;
}
.col2{
	width:calc(200% / 12);
}
.col12{
	width:100%;
}
.col8{
	margin:0 auto;
}
.col3{
	width:25%;
}
.col9{
	width:75%;
}
.col4{
	width:calc(100% / 3);
}
.padding{
	padding:40px 0 35px;
}
.get-in-touch{
	background-color:#2f4052;
	
	
}
 
.secheading{
	width:100%;
	text-align:center;
}
.w-8{
	width:calc(800% / 12);
	
}
.secpara{
	margin:0 auto;
	text-align:center;
	
}
.sectionlable{
	margin:0 auto;
	text-align:center;
}
.search{
	/* position:absolute; */
	/* top:50px; */
	/* right:20px; */
	padding: 20px;
}
.navbar ul li a.active,
.navbar ul li a:hover,
.navbar ul li a:focus,
.navbar ul li a:active{
	font-weight:lighter;
}	


